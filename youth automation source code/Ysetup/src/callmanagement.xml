<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
"http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="callmanagement" namespace="/" extends="struts-app">
	
		<action name="callmanagementload"
			class="com.iyouth.common.callmanagement.action.CallManagementAction" method="loadCallManagement">
			<result name="loadpage">/pages/callManagement/callManagement.jsp</result>
		</action>
		
		<action name="callmanagementsearch"
			class="com.iyouth.common.callmanagement.action.CallManagementAction" method="searchCallManagement">
			<result name="loadpage">/pages/callManagement/callManagement.jsp</result>
		</action>
		
		<action name="fetchstudentdetails"
			class="com.iyouth.common.callmanagement.action.CallManagementAction" method="fetchCallManagement">
			<result name="loadpage">/pages/callManagement/callManagement_temppage.jsp</result>
		</action>
	</package>
</struts>
